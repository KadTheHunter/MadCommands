cmd create info Server info
cmd add info %gui%[\"type\"] = \"form\"
cmd add info %gui%[\"title\"] = \"§a§lInfo\"
cmd add info %gui%[\"content\"] = \"Click to open | GUI developed by punjana12\"
cmd add info %b0%[\"text\"] = \"§bServer/Social Media\"
cmd add info %b1%[\"text\"] = \"§eRanks & Other Features\"
cmd add info %b2%[\"text\"] = \"§dUpdates\"
cmd add info %bs%[] = %b0%
cmd add info %bs%[] = %b1%
cmd add info %bs%[] = %b2%
cmd add info %gui%[\"buttons\"] = %bs%
cmd add info %res% = dialog(%p%, %gui%)
cmd add info if %res% = 0 then goto 16
cmd add info if %res% = 1 then goto 25
cmd add info if %res% = 1 then goto 32
cmd add info exit
