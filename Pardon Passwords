cmd create pardon Allows the specified player to use this server.
cmd add pardon let %allowed% = array(\"VesperSoup48737\", \"ATskullXD5\", \"PotatoSheepMilk\", \"DRCH16\")
cmd add pardon if %p% in %allowed% then goto 5
cmd add pardon message %p% §cYou are not allowed to use this command!
cmd add pardon exit cmd add pardon let %target% = %args%[1]
cmd add pardon if varset(%target%) = %false% then message %p% Usage: /pardon <player>
cmd add pardon if varset(%target%) = %false% then exit
cmd add pardon if %target% = \"NightSavageYT\" then goto 100
cmd add pardon if %target% = \"WolfPrincess64\" then goto 200
cmd add pardon if %target% = \"EleWolf101\" then goto 300
cmd add pardon /pardon %target%
cmd add pardon exit
cmd insert pardon 100 let %password% = %args%[2]
cmd add pardon if varset(%password%) = %false% then message %p% \"§cPlease enter a valid password!\"
cmd add pardon if varset(%password%) = %false% then exit
cmd add pardon if %password% != \"RYANweedPORN-08-17\" then message %p% \"§cIncorrect password!\"
cmd add pardon if %password% != \"RYANweedPORN-08-17\" then exit
cmd add pardon /pardon %target%
cmd add pardon exit
cmd insert pardon 200 let %password% = %args%[2]
cmd add pardon if varset(%password%) = %false% then message %p% \"§cPlease enter a valid password!\"
cmd add pardon if varset(%password%) = %false% then exit
cmd add pardon if %password% != \"#AntiDarzy-11-17\" then message %p% \"§cIncorrect password!\"
cmd add pardon if %password% != \"#AntiDarzy-11-17\" then exit
cmd add pardon /pardon %target%
cmd add pardon exit
cmd insert pardon 300 let %password% = %args%[2]
cmd add pardon if varset(%password%) = %false% then message %p% \"§cPlease enter a valid password!\"
cmd add pardon if varset(%password%) = %false% then exit
cmd add pardon if %password% != \"LabDestroyingSwine\" then message %p% \"§cIncorrect password!\"
cmd add pardon if %password% != \"LabDestroyingSwine\" then exit
cmd add pardon /pardon %target%
